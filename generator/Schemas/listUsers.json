{
	"definitions": {},
	"$schema": "http://json-schema.org/draft-07/schema#", 
	"$id": "https://example.com/object1587561526.json", 
	"title": "Root", 
	"type": "object",
	"required": [
		"page",
		"per_page",
		"total",
		"total_pages",
		"data",
		"ad"
	],
	"properties": {
		"page": {
			"$id": "#root/page", 
			"title": "Page", 
			"type": "integer",
			"examples": [
				2
			],
			"default": 0
		},
		"per_page": {
			"$id": "#root/per_page", 
			"title": "Per_page", 
			"type": "integer",
			"examples": [
				6
			],
			"default": 0
		},
		"total": {
			"$id": "#root/total", 
			"title": "Total", 
			"type": "integer",
			"examples": [
				12
			],
			"default": 0
		},
		"total_pages": {
			"$id": "#root/total_pages", 
			"title": "Total_pages", 
			"type": "integer",
			"examples": [
				2
			],
			"default": 0
		},
		"data": {
			"$id": "#root/data", 
			"title": "Data", 
			"type": "array",
			"default": [],
			"items":{
				"$id": "#root/data/items", 
				"title": "Items", 
				"type": "object",
				"required": [
					"id",
					"email",
					"first_name",
					"last_name",
					"avatar"
				],
				"properties": {
					"id": {
						"$id": "#root/data/items/id", 
						"title": "Id", 
						"type": "integer",
						"examples": [
							7
						],
						"default": 0
					},
					"email": {
						"$id": "#root/data/items/email", 
						"title": "Email", 
						"type": "string",
						"default": "",
						"examples": [
							"michael.lawson@reqres.in"
						],
						"pattern": "^.*$"
					},
					"first_name": {
						"$id": "#root/data/items/first_name", 
						"title": "First_name", 
						"type": "string",
						"default": "",
						"examples": [
							"Michael"
						],
						"pattern": "^.*$"
					},
					"last_name": {
						"$id": "#root/data/items/last_name", 
						"title": "Last_name", 
						"type": "string",
						"default": "",
						"examples": [
							"Lawson"
						],
						"pattern": "^.*$"
					},
					"avatar": {
						"$id": "#root/data/items/avatar", 
						"title": "Avatar", 
						"type": "string",
						"default": "",
						"examples": [
							"https://s3.amazonaws.com/uifaces/faces/twitter/follettkyle/128.jpg"
						],
						"pattern": "^.*$"
					}
				}
			}

		},
		"ad": {
			"$id": "#root/ad", 
			"title": "Ad", 
			"type": "object",
			"required": [
				"company",
				"url",
				"text"
			],
			"properties": {
				"company": {
					"$id": "#root/ad/company", 
					"title": "Company", 
					"type": "string",
					"default": "",
					"examples": [
						"StatusCode Weekly"
					],
					"pattern": "^.*$"
				},
				"url": {
					"$id": "#root/ad/url", 
					"title": "Url", 
					"type": "string",
					"default": "",
					"examples": [
						"http://statuscode.org/"
					],
					"pattern": "^.*$"
				},
				"text": {
					"$id": "#root/ad/text", 
					"title": "Text", 
					"type": "string",
					"default": "",
					"examples": [
						"A weekly newsletter focusing on software development, infrastructure, the server, performance, and the stack end of things."
					],
					"pattern": "^.*$"
				}
			}
		}

	}
}